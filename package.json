{
  "name": "ts-nextjs-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze:unused": "next-unused",
    "analyze": "cross-env ANALYZE=true yarn run build",
    "unimported": "unimported",
    "find-unused-exports": "find-unused-exports --moduyle-glob \"**/*.{ts,tsx}\" --resolve-file-extensions ts,tsx --resolve-index-files",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "postbuild": "next-sitemap",
    "prepare": "husky install"
  },
  "dependencies": {
    "@primer/octicons-react": "^16.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "clsx": "^1.1.1",
    "memoize-one": "^6.0.0",
    "next": "^12.1.0",
    "next-seo": "^5.1.0",
    "next-themes": "^0.1.1",
    "plur": "^5.1.0",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "swr": "^1.2.2",
    "tailwind-merge": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@next/bundle-analyzer": "^12.1.0",
    "@svgr/webpack": "^6.2.1",
    "@tailwindcss/forms": "^0.4.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/plur": "^3.1.0",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.39",
    "@types/tailwindcss": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "autoprefixer": "^10.4.2",
    "babel-jest": "27.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "find-unused-exports": "^3.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-docblock": "27.5.1",
    "jest-runner": "27.5.1",
    "jest-runner-groups": "2.1.0",
    "lint-staged": "^12.3.4",
    "next-sitemap": "^2.5.1",
    "next-unused": "^0.0.6",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "react-test-renderer": "^17.0.2",
    "standard-version": "^9.3.2",
    "tailwindcss": "^3.0.23",
    "typescript": "4.5.5",
    "unimported": "^1.19.1"
  },
  "next-unused": {
    "alias": {
      "@/*": [
        "src/*"
      ]
    },
    "debug": true,
    "include": [
      "src",
      "src/modules",
      "src/ui"
    ],
    "exclude": [
      ".test.",
      ".unit.",
      ".spec."
    ],
    "entrypoints": [
      "src/pages",
      "src/modules"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  }
}
