{
	"name": "create-nextify-app",
	"version": "0.0.2-7",
	"description": "React apps using Next.js and Emotion.",
	"keywords": [
		"nextjs",
		"next",
		"react",
		"javascript",
		"typescript",
		"ts",
		"framework",
		"emotion",
		"styles",
		"css",
		"styling",
		"css-in-js",
		"workflow"
	],
	"homepage": "https://www.nextify.me",
	"repository": "https://github.com/luangjokaj/nextify",
	"exports": "./index.js",
	"resolutions": {
		"**/trim": "^1.0.0",
		"**/glob-parent": "^5.1.2",
		"**/immer": "^9.0.6"
	},
	"scripts": {
		"dev": "next dev",
		"build": "yarn clean:dist && next build",
		"start": "next start",
		"export": "next export",
		"lint": "eslint --ignore-path .gitignore --cache --ext .js,.jsx,.ts,.tsx ./",
		"lint:fix": "yarn run lint --fix",
		"type-check": "tsc",
		"clean:dist": "rimraf .next .eslintcache yarn-error.log tsconfig.tsbuildinfo .next/cache/**",
		"clean": "yarn clean:dist && rimraf node_modules",
		"nude-node": "rimraf node_modules yarn.lock",
		"type-watch": "tsc --watch",
		"format": "prettier --write --config .prettierrc ./src",
		"analyze": "ANALYZE=true next build",
		"analyze:server": "BUNDLE_ANALYZE=server next build",
		"analyze:browser": "BUNDLE_ANALYZE=browser next build",
		"sb": "yarn storybook",
		"storybook": "STORYBOOK=true start-storybook -h localhost -p 6006 --no-manager-cache",
		"build-storybook": "STORYBOOK=true build-storybook -s ./public",
		"check": "yarn type-check && yarn lint && yarn build"
	},
	"author": "Lesley Chang",
	"license": "MIT",
	"dependencies": {
		"@emotion/react": "^11.7.1",
		"@emotion/styled": "^11.6.0",
		"@react-hookz/web": "^12.0.0",
		"@supabase/supabase-js": "^1.28.5",
		"@tailwindcss/line-clamp": "^0.3.0",
		"classnames": "^2.3.1",
		"express": "^4.17.2",
		"next": "12.0.7",
		"next-compose-plugins": "^2.2.1",
		"next-seo": "^4.28.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"swr": "^1.1.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "eslint --ext .ts,.tsx --config .eslintrc"
		}
	},
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@babel/plugin-transform-react-jsx": "^7.16.5",
		"@babel/preset-typescript": "^7.16.5",
		"@babel/runtime": "^7.16.5",
		"@emotion/babel-plugin": "^11.7.1",
		"@emotion/eslint-plugin": "^11.2.0",
		"@next/bundle-analyzer": "12.0.7",
		"@storybook/preset-typescript": "^3.0.0",
		"@storybook/react": "^6.4.9",
		"@storybook/source-loader": "^6.4.9",
		"@testing-library/dom": "^8.11.1",
		"@testing-library/react": "^12.1.2",
		"@types/jest": "^27.0.3",
		"@types/node": "^17.0.1",
		"@types/react": "^17.0.20",
		"@types/react-dom": "^17.0.9",
		"@types/uuid": "^8.3.3",
		"@typescript-eslint/eslint-plugin": "^5.8.0",
		"@typescript-eslint/parser": "^5.8.0",
		"autoprefixer": "^10.4.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.2",
		"babel-preset-react-app": "^10.0.1",
		"eslint": "^8.5.0",
		"eslint-config-airbnb-typescript": "^16.1.0",
		"eslint-config-next": "12.0.7",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.25.1",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-sonarjs": "^0.11.0",
		"husky": "^7.0.4",
		"jest": "^27.4.5",
		"lint-staged": "^12.1.3",
		"postcss": "^8.4.5",
		"prettier": "^2.5.1",
		"rimraf": "^3.0.2",
		"tailwindcss": "^3.0.7",
		"typescript": "^4.5.4",
		"uuid": "^8.3.2",
		"webpack-merge": "^5.8.0"
	},
	"jest": {
		"setupFilesAfterEnv": [
			"./jest.setup.ts"
		],
		"testPathIgnorePatterns": []
	}
}
